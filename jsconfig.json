{
  "compilerOptions": {
    "checkJs": false,
    "target": "ES2022",
    "module": "ES2022",
    "moduleResolution": "Bundler",
    "baseUrl": ".",
    "types": ["node"],
    "paths": {
      "~/*": [
        "app/*"
      ]
    }
  },
  "include": [
    "./**/*.d.ts",
    "./**/*.js",
    "./**/*.jsx",
    ".react-router/types/**/*"
  ]
}